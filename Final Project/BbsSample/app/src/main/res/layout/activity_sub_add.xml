<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".subscribe.SubAddActivity">

    <androidx.appcompat.widget.Toolbar
            android:id="@+id/subAdd_toolbar"
            android:layout_width="match_parent"
            android:layout_height="?android:attr/actionBarSize"
            app:titleTextColor="@color/meta_grey01"
            app:titleTextAppearance="@style/TitleBarTextAppearance"
            android:background="@color/meta_white" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"/>
    <TextView
            android:id="@+id/subAdd_textView14"
            android:text="구독 신청" android:gravity="center_vertical" android:textSize="20dp"
            android:fontFamily="@font/font_content_bold"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.012" android:layout_marginStart="24dp"
            android:layout_marginEnd="24dp" app:layout_constraintTop_toBottomOf="@+id/subAdd_toolbar"/>
    <TextView
            android:id="@+id/subAdd_textView15"
            android:text="유형" android:gravity="center_vertical" android:textSize="20dp"
            android:fontFamily="@font/font_content_regular"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/subAdd_textView14" android:layout_marginTop="50dp"
            app:layout_constraintHorizontal_bias="0.011" android:layout_marginStart="24dp"/>
    <Spinner
            android:id="@+id/subAdd_TypeSpinn"
            android:layout_width="520dp"
            android:layout_height="50dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.178"
            app:layout_constraintTop_toBottomOf="@+id/subAdd_textView15"
            android:layout_marginStart="24dp" android:layout_marginEnd="24dp"/>
    <TextView
            android:id="@+id/subadd_textView16"
            android:text="구독 기간" android:gravity="center_vertical" android:textSize="20dp"
            android:fontFamily="@font/font_content_regular"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/subAdd_TypeSpinn" android:layout_marginTop="50dp"
            app:layout_constraintHorizontal_bias="0.012" android:layout_marginStart="24dp"/>
    <Spinner
            android:id="@+id/subAdd_PeriodSpinn"
            android:layout_width="520dp"
            android:layout_height="50dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.178"
            android:layout_marginStart="24dp" app:layout_constraintTop_toBottomOf="@+id/subadd_textView16"
            android:layout_marginEnd="24dp"/>
    <TextView
            android:text="신청 시간대" android:gravity="center_vertical" android:textSize="20dp"
            android:fontFamily="@font/font_content_regular"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/subadd_textView17"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/subAdd_PeriodSpinn" android:layout_marginTop="50dp"
            app:layout_constraintHorizontal_bias="0.012" android:layout_marginStart="24dp"/>
    <CheckBox
            android:text="아침 Morning" android:textSize="15dp"
            android:fontFamily="@font/font_content_regular"
            android:layout_width="520dp"
            android:layout_height="65dp" android:id="@+id/subAdd_morningCheck"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/subadd_textView17" android:layout_marginStart="24dp"
            android:layout_marginEnd="24dp" app:layout_constraintHorizontal_bias="0.187"/>
    <CheckBox
            android:text="점심 Lunch" android:textSize="15dp"
            android:fontFamily="@font/font_content_regular"
            android:layout_width="520dp"
            android:layout_height="65dp" android:id="@+id/subAdd_lunchCheck"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="24dp"
            android:layout_marginEnd="24dp" app:layout_constraintHorizontal_bias="0.187"
            app:layout_constraintTop_toBottomOf="@+id/subAdd_morningCheck"/>
    <CheckBox
            android:text="저녁 Dinner" android:textSize="15dp"
            android:fontFamily="@font/font_content_regular"
            android:layout_width="520dp"
            android:layout_height="65dp" android:id="@+id/subAdd_dinnerCheck"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="24dp"
            android:layout_marginEnd="24dp"
            app:layout_constraintTop_toBottomOf="@+id/subAdd_lunchCheck" app:layout_constraintHorizontal_bias="0.187"/>
    <CheckBox
            android:text="간식 Snack" android:textSize="15dp"
            android:fontFamily="@font/font_content_regular"
            android:layout_width="520dp"
            android:layout_height="65dp" android:id="@+id/subAdd_snackCheck"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="24dp"
            android:layout_marginEnd="24dp"
            app:layout_constraintHorizontal_bias="0.187"
            app:layout_constraintTop_toBottomOf="@+id/subAdd_dinnerCheck"/>
    <Button
            android:text="Google Store 결제"
            android:fontFamily="@font/font_content_regular"
            android:layout_width="520dp"
            android:layout_height="50dp" android:id="@+id/subAdd_addBtn"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.187" android:layout_marginStart="24dp"
            android:layout_marginEnd="24dp"
            app:layout_constraintTop_toBottomOf="@+id/subAdd_snackCheck" app:layout_constraintVertical_bias="0.155"/>
    <Button
            android:id="@+id/subAdd_payAPIBtn"
            android:text="CARD 결제"
            android:fontFamily="@font/font_content_regular"
            android:layout_width="520dp"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/subAdd_addBtn" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.187" app:layout_constraintVertical_bias="0.14"
            android:layout_marginStart="24dp" android:layout_marginEnd="24dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>